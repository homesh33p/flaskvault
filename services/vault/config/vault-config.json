{
  "storage": {
    "file": {
      "path": "/vault/file"
    }
  },
  "log": {
    "file": {
      "path": "/var/log/vault.log"
    }
  },  
  "listener": [
    {
      "tcp":
      {
        "address": "0.0.0.0:8200",
        "tls_disable": true
      }
    }
  ],
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "api_addr" : "http://0.0.0.1:8200",
  "ui": true,
  "disable_mlock" : true
}